pipeline {

    agent any

    stages {

        stage('Build') {

            steps {
                sh 'echo "Build Completed."'
            }
        }
        stage('Upload Artifact') {

            steps {
                sh 'echo "Artifact Uploaded"'
            }
        }
        stage('Deploy') {

            steps {
                sh 'echo "Deploy Completed"'
            }
        }
    }
}